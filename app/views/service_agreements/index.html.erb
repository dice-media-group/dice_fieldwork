<h1>Service Agreements#index</h1>
<p>Find me in app/views/notes/index.html.erb</p>
<h1 id="service_agreemeents">Service Agreements</h1>
<div>
    <div >
        <% if @agreements.count > 0 %>
            <% @agreements.each do |agreement| %>
                <%= "Service Agreement created at#{agreement.created_at}" %>
            <% end %>
        <% else %>
            <p>This account does not have a service agreement yet.</p>
            <p><%= link_to "Create Service Agreement", new_account_service_agreement_path(@account) %></p>
        <% end %>
    </div>
</div>
